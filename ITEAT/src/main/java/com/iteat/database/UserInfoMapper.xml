<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iteat.domain.UserInfoDAO">
	<insert id="insertUif" parameterType="com.iteat.domain.UserInfo">
		insert into user_info values(#{uif_id},#{uif_pw},#{uif_nick},sysdate,1,#{uif_stack},#{uif_blog},#{uif_edu},#{uif_etc},'G')
	</insert>
</mapper>